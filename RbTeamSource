local but = game.Players.LocalPlayer.PlayerGui.GUI.TeamSelection.Buttons["Or"]:Clone()
while wait() do
for hue = 0, 1, 1/30 do
   but.ImageColor3 = Color3.fromHSV(hue, 1, 1)
   wait(0.01)
end
but.Name = "Rainbow"
but.Position = UDim2.new(1, 150, 0, 0)
but.Visible  = true
but.Parent = game.Players.LocalPlayer.PlayerGui.GUI.TeamSelection.Buttons
but.MouseButton1Click:connect(function()
 local args = {
    [1] = "TRC",
    [2] = "L"
}

game:GetService("ReplicatedStorage").Events.JoinTeam:FireServer(unpack(args))
wait(0.05)
 spawn(function()

    while wait() do

for hue = 0, 1, 1/30 do
   game.workspace[game.Players.LocalPlayer.Name].Cap.Handle.Color = Color3.fromHSV(hue, 1, 1)
wait(0.03)
end

end
end)
  spawn(function()
    
      while wait() do

for hue = 0, 1, 1/30 do
   game.workspace[game.Players.LocalPlayer.Name].LeftUpperArm.Color = Color3.fromHSV(hue, 1, 1)
wait(0.03)
end
end
end)
spawn(function()
        
    while wait() do

for hue = 0, 1, 1/30 do
   game.workspace[game.Players.LocalPlayer.Name].RightUpperArm.Color = Color3.fromHSV(hue, 1, 1)
wait(0.03)
end

end
end)
spawn(function()
         
    while wait() do         
for hue = 0, 1, 1/30 do
   game.workspace[game.Players.LocalPlayer.Name].UpperTorso.Color = Color3.fromHSV(hue, 1, 1)
wait(0.03)
end

end
end)
game.Players.LocalPlayer.Status.Team.Value="TOC"
game.Players.LocalPlayer.Status.Team.Value="TOC"
game.Players.LocalPlayer.Status.Team.Value="TOC"
game.Players.LocalPlayer.Status.Team.Value="TOC"
game.Players.LocalPlayer.Status.Team.Value="TOC"
wait(0.19)
wait(0.19)
game.Players.LocalPlayer.Status.Team.Value="TOC"
wait(1)
game.Players.LocalPlayer.Status.Team.Value="TOC"
game.Players.LocalPlayer.Status.Team.Value="TOC"
game.Players.LocalPlayer.Status.Team.Value="TOC"
game.Players.LocalPlayer.Status.Team.Value="TOC"
game.Players.LocalPlayer.Status.Team.Value="TOC"
game.Players.LocalPlayer.Status.Team.Value="TOC"
wait(0.1765785653452)
game.Players.LocalPlayer.Status.Team.Value="TOC"
game.Players.LocalPlayer.TeamColor = BrickColor.new("Flame reddish orange")
game.Players.LocalPlayer.Character.UpperTorso.BrickColor = BrickColor.new("Flame reddish orange")
game.Players.LocalPlayer.Character.LeftUpperArm.BrickColor = BrickColor.new("Flame reddish orange")
game.Players.LocalPlayer.Character.LeftUpperLeg.BrickColor = BrickColor.new("Flame reddish orange")
game.Players.LocalPlayer.Character.RightUpperLeg.BrickColor = BrickColor.new("Flame reddish orange")
game.Players.LocalPlayer.Character.RightUpperArm.BrickColor = BrickColor.new("Flame reddish orange")
game.Players.LocalPlayer.Character.Cap.Handle.BrickColor = BrickColor.new("Flame reddish orange")
game.Players.LocalPlayer.PlayerGui.GUI.TeamSelection.Visible = false
game.Players.LocalPlayer.PlayerGui.GUI.BottomFrame.Visible = false
wait(0.09)
wait(2)
game.Players.LocalPlayer.Ragdoll.UpperTorso.BrickColor = BrickColor.new("Flame reddish orange")
game.Players.LocalPlayer.Ragdoll.LeftUpperArm.BrickColor = BrickColor.new("Flame reddish orange")
game.Players.LocalPlayer.Ragdoll.LeftUpperLeg.BrickColor = BrickColor.new("Flame reddish orange")
game.Players.LocalPlayer.Ragdoll.RightUpperLeg.BrickColor = BrickColor.new("Flame reddish orange")
game.Players.LocalPlayer.Ragdoll.RightUpperArm.BrickColor = BrickColor.new("Flame reddish orange")
game.Players.LocalPlayer.Ragdoll.Cap.Handle.BrickColor = BrickColor.new("Flame reddish orange")
while game.RunService.Stepped:wait()do
    for i=1,6 do
        if workspace.KillFeed[tostring(i)]:findFirstChild("Assist").Value == game.Players.LocalPlayer.Name or workspace.KillFeed[tostring(i)]:findFirstChild("Victim").Value == game.Players.LocalPlayer.Name or workspace.KillFeed[tostring(i)]:findFirstChild("Killer").Value == game.Players.LocalPlayer.Name then
       
             if workspace.KillFeed[tostring(i)]:findFirstChild("Assist").Value==game.Players.LocalPlayer.Name then
                workspace.KillFeed[tostring(i)]:findFirstChild("Assist").TeamColor.Value=Color3.fromRGB(207, 96, 36)
           elseif workspace.KillFeed[tostring(i)]:findFirstChild("Victim").Value==game.Players.LocalPlayer.Name then
                        workspace.KillFeed[tostring(i)]:findFirstChild("Victim").TeamColor.Value=Color3.fromRGB(207, 96, 36)
        
        elseif workspace.KillFeed[tostring(i)]:findFirstChild("Killer").Value==game.Players.LocalPlayer.Name then
                            workspace.KillFeed[tostring(i)]:findFirstChild("Killer").TeamColor.Value=Color3.fromRGB(207, 96, 36)

        end
        end
end
end
end)
end






















